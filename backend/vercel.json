{
  "version": 2,
  "name": "bagniverse-backend",
  "builds": [{ "src": "server.js", "use": "@vercel/node" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        }
      ]
    }
  ],
  "redirects": [{ "source": "/(.*)", "destination": "/server.js" }]
}
